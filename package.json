{
  "name": "email-labeler-agent",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "create": "clasp create --type standalone --title \"Gmail Labeler (Mail Screener)\" --rootDir ./src",
    "push": "clasp push",
    "pull": "clasp pull",
    "open": "clasp open-script",
    "status": "clasp status",
    "logs": "clasp logs --watch",
    "open:logs": "clasp open-logs",
    "version": "clasp version",
    "version:stable": "clasp version \"stable-$(date +%Y%m%d-%H%M%S)\"",
    "deploy": "npm run version:stable && clasp deploy --description \"Production deployment $(date)\"",
    "deploy:full": "npm run deploy && npm run trigger:install",
    "trigger:install": "clasp run installTrigger",
    "trigger:delete": "clasp run deleteExistingTriggers_"
  },
  "devDependencies": {}
}


